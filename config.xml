<?xml version='1.0' encoding='utf-8'?>
<widget id="com.steps.ussap" version="3.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>UST Support Staff Portal</name>
    <description>
    </description>
    <author email="myust.steps@gmail.com" href="">
        STEPS
    </author>
    <content src="index.html" />
    <preference name="android-version" value="7.0.0" />
    <preference name="ios-version" value="^4.5.4" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="23" />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name="android-compileSdkVersion" value="28" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <preference name="Fullscreen" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="CustomURLSchemePluginClearsAndroidIntent" value="true" />
    <preference name="SplashScreen" value="none" />
    <plugin name="cordova-plugin-battery-status" source="npm" spec="~1.1.1" />
    <plugin name="cordova-plugin-camera" source="npm" spec="~2.1.1" />
    <plugin name="cordova-plugin-contacts" source="npm" spec="~2.0.1" />
